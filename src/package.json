    "pguid": "https://github.com/shukriadams/pguid.git#0.0.1",
    "handlebars-layouts": "3.1.4"
  },
  "jspm": {
    "registry": "npm",
    "directories": {
      "baseURL": "client/app",
      "packages": "client/lib"
    },
    "dependencies": {
      "babel": "babel-core@5.8.38",
      "babel-plugin-transform-react-jsx": "6.24.1",
      "babel-runtime": "5.8.38",
      "classnames": "2.2.5",
      "core-js": "2.6.5",
      "history": "4.7.2",
      "localforage": "1.5.6",
      "react": "16.6.3",
