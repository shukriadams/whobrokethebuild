{
  "name": "whobrokethebuild",
  "license": "UNLICENSED",
  "description": "Mvc boilerplate. Express + ReactJS. JSPM",
  "repository": "https://github.com/shukriadams/whobrokethebuild",
  "scripts": {
    "start": "node index",
    "profile": "node --inspect=0.0.0.0:3001 index",
    "build": "npm run client && npm run assets",
    "assets": "node ./build/build-all-assets",
    "client": "sh ./build/bundle.sh",
    "test": "node ./tests/run-all",
    "icons" : "cd build && node icons",
    "dev": "node ./build/dev.js"
  },
  "devDependencies": {
    "mocha": "7.1.0",
    "fileconcat": "2.0.1",
    "concat": "1.0.3",
    "clean-css": "4.2.1",
    "chokidar": "2.1.5",
    "node-sass": "4.12.0",
    "minimist": "1.2.5",
    "cross-spawn": "6.0.5",
    "webfonts-generator": "0.4.0",
    "madscience-node-assert": "https://github.com/shukriadams/node-assert.git#0.0.2",
    "node-sass-runner": "https://github.com/shukriadams/node-sass-runner.git#0.0.5"
  },
  "dependencies": {
    "@types/node": "14.14.25",
    "cache-require-paths": "0.3.0",
    "colors": "1.4.0",
    "randomstring": "1.1.5",
    "glob": "7.1.1",
    "slack": "11.0.2",
    "nodemailer": "5.1.1",
    "cron": "1.8.2",
    "express-useragent": "1.0.13",
    "node-cache": "4.1.1",
    "node-ad-tools": "1.2.1",
    "lodash.merge" : "4.6.2",
    "fs-extra": "9.0.0",
    "js-yaml": "3.13.1",
    "express": "4.16.3",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.4",
    "custom-env": "1.0.0",
    "handlebars": "4.4.3",
    "md5-file": "4.0.0",
    "socket.io": "2.1.1",
    "s-ago": "2.1.0",
    "urljoin": "0.1.5",
    "jsonfile": "6.0.1",
    "statman-stopwatch": "2.11.1",
    "timebelt": "https://github.com/shukriadams/timebelt.git#0.1.9",
    "winston-wrapper": "https://github.com/shukriadams/winston-wrapper.git#0.1.0",
    "madscience-httputils": "https://github.com/shukriadams/node-httpUtils.git#0.0.4",
    "madscience-node-exec": "https://github.com/shukriadams/node-exec.git#0.0.6",
