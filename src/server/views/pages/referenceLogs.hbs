{{#extend "layout"}}
    {{#content "body"}}
        <div>
            <h1>Reference Logs</h1>
            
            {{#if referenceLogs.error}}
                {{referenceLogs.error}}
            {{else}}
                <ul class="">
                    {{#each referenceLogs.items}}
                        <li class="list-item list-item--textContent">
                            <a href="/referenceLogs/{{this}}">{{this}}</a>
                        </li>
                    {{/each}}
                </ul>

                {{{pager baseUrl referenceLogs }}}

            {{/if}}

        </div>
    {{/content}}
{{/extend}}


